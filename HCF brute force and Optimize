//Brute Force
#include <iostream> 
using namespace std;

int main() {
    int a, b, hcf = 1;
    cout << "Enter two numbers: ";
    cin >> a >> b;
    for (int i = 1; i <= a && i <= b; i++) {
        if (a % i == 0 && b % i == 0) {
            hcf = i;
        }
    }
    cout << "HCF = " << hcf;
    return 0;
}

//Optimize using Euclidean
#include <iostream>
using namespace std;
int main() 
{
  int a, b;
  cout << "Enter two numbers: ";
  cin >> a >> b;
  int x = a, y = b;
  while (y != 0) {
    int temp = y;
    y = x % y;
    x = temp;
  }
  cout << "HCF: " << x << endl;
  return 0;
}
